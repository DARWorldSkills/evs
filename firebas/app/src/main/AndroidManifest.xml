<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.davidpopayan.sena.evs">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/un"
        android:label="@string/app_name"
        android:roundIcon="@drawable/un"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".controllers.Splash"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".controllers.MenuP" />
        <activity android:name=".controllers.PrimerForm" />
        <activity android:name=".controllers.Encuesta" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".controllers.EncuestaDos" />
        <activity android:name=".controllers.Resultados" />
        <activity android:name=".controllers.datosPersonales" />
        <activity android:name=".controllers.EncuestaTres" />
        <activity android:name=".controllers.IniciarSesion" />
        <activity android:name=".controllers.BloqueoUsuarios" />
        <activity android:name=".controllers.CrearUsuario"></activity>
    </application>

</manifest>